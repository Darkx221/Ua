def sex():
	facebook_version = f"{random.randint(75, 750)}.{random.randint(0, 0)}.{random.randint(0, 0)}.{random.randint(1, 99)}.{random.randint(100, 1000)}"
	fbbv = str(random.randint(10000000, 66666666))
	fbrv = str(random.randint(11111111,99999999))
	density = random.choice(['2.0', '2.5', '3.0'])
	width = random.choice(["720", "1080", "1280"])
	height = random.choice(["720", "1080", "1280", "1440", "1920"])
	ua = f"[FBAN/FB4A;FBAV/{str(facebook_version)};FBBV/{str(fbbv)};[FBAN/FB4A;FBAV/269.0.0.50.127;FBBV/212783074;FBDM/{{density={density},width={width},height={height}}};FBLC/en_US;FBRV/{str(fbrv)};FBCR/Verizon ;FBMF/samsung;FBBD/samsung;FBPN/com.facebook.katana;FBDV/SM-N976V;FBSV/10;FBOP/1;FBCA/arm64-v8a:;]"
